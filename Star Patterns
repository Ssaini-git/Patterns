public class Solution {
    public static void nBinaryTriangle(int n) {
        // Write your code here.
        
        for( int i = 1; i <= n; i++ ){
            int c = i % 2 == 0 ? 0 : 1;
            for( int j = 1; j<= i; j++ ){
                System.out.print(c+" ");
                c = c == 1 ? 0 : 1; 
            }
            System.out.println();
        }
    }
}
