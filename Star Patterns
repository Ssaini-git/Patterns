public class Solution {
    public static void numberCrown(int n) {
        // Write your code here.
        for( int i = 1; i<= n; i ++ ){
		    
		    for( int j = 1; j <= i; j++){
		        System.out.print(j+" ");
		    }
		    for( int j = 1; j<=2 * (n-i); j++){
		        System.out.print(" ");
		    }
		    int c = i;
		    for( int j = 1; j <= i; j++){
		         System.out.print(c+" ");
		         c--;
		    }
		    	System.out.println();
		}
    }
}
