*         *
public class Solution {
    public static void symmetry(int n) {
        // Write your code here
     for( int i =1; i<= 2*n; i++){
        if( i <= n){
        for( int j = 1; j<=i; j++){
            System.out.print("* ");
        }
        for(int j = 1; j <= n-i; j++){
            System.out.print(" ");
        }
        for( int j = 1; j <=i; j++){
            System.out.print("* ");
        }
        }
        else{
          for( int j =1; j<= 2 *n-i; j++){
              System.out.print("* ");
          }
          for( int j = 1; j< 2*(i-n); j++){
              System.out.print(" ");
          }
         for( int j =1; j<= 2 *n-i; j++){
              System.out.print("* ");
          }
        }
        System.out.println();
    }
    }
}
